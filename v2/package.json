{
  "name": "v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-css": "node generate-css.js && cat generated.css input.css > temp.css && postcss temp.css -o output.css && rm temp.css",
    "dev": "concurrently \"npm run build-css -- --watch\" \"vite\"",
    "build": "concurrently \"npm run build-css\" \"vite build\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "vite": "^5.2.0"
  }
}
